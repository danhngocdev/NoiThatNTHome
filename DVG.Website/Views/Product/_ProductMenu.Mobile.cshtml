@using DVG.WIS.PublicModel;
@model List<ProductFEListModel>

@if (Model != null && Model.Any())
{
    <ul class="dropdown-menu-mobi">
        @foreach (var item in Model)
        {
            <li>
                <a class="@(Request.RawUrl.ToString().Contains(item.Link) ? "active":"")" href="@item.Link" title="@item.Name"><i class="pull-left pd-right-10 icon-minus"></i>@item.Name</a>
            </li>
        }
    </ul>
}
